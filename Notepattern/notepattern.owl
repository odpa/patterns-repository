        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/andreapoltronieri/notepattern/SymbolicNote"/>
        <rdfs:range rdf:resource="https://purl.org/andreapoltronieri/scorepart/Section"/>

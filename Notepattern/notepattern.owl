<?xml version="1.0"?>
<rdf:RDF xmlns="https://purl.org/andreapoltronieri/notepattern/"
     xml:base="https://purl.org/andreapoltronieri/notepattern/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:np="https://purl.org/andreapoltronieri/notepattern/"
     xmlns:sp="https://purl.org/andreapoltronieri/scorepart/"
     xmlns:mto="https://archive.org/services/purl/purl/ontology/mto/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:mobj="https://purl.org/andreapoltronieri/musicalobject/"
     xmlns:opla="http://ontologydesignpatterns.org/opla/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="https://purl.org/andreapoltronieri/notepattern/">
        <owl:imports rdf:resource="https://purl.org/andreapoltronieri/musicalobject"/>
        <owl:imports rdf:resource="https://purl.org/andreapoltronieri/scorepart"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Andrea Poltronieri (University of Bologna)</dc:creator>
        <dc:description xml:lang="en">The Note Pattern models a music note both in relation to a music score and in relation to the musical object produced by playing such note.</dc:description>
        <dc:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
        <dc:title xml:lang="en">Note Pattern</dc:title>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <cpannotationschema:coversRequirements xml:lang="en">what is the name of a note?, what part of the score does a note belong to?, what are the dynamic indications referring to a note in the score?, what is the fundamental frequency of a note?, what are the different frequencies that make up the spectrum of a note?, what is the duration in seconds of a note, in a given performance?, how is the envelope of a note shaped?</cpannotationschema:coversRequirements>
        <cpannotationschema:hasComponent rdf:resource="http://purl.org/net/nary-relation"/>
        <cpannotationschema:hasComponent rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl"/>
        <cpannotationschema:hasIntent xml:lang="en">The Music Note ODP models a symbolic note and its symbolic attributes.</cpannotationschema:hasIntent>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?accidental
WHERE {?note rdf:type ?type.
  ?type rdfs:subClassOf* np:SymbolicNote
  OPTIONAL {?note np:hasAccidental ?accidental}}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?durationSec
WHERE {?note np:hasRealisation ?musicalObj .  
  ?musicalObj mop:hasDuration ?duration . 
  ?duration mop:hasDurationInSeconds ?durationSec}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?foundamFreq
WHERE {?note np:hasRealisation ?musicalObj .  
  ?musicalObj mop:hasFrequency ?foundamFreq . 
  ?foundamFreq a mop:FundamentalFrequency.}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?freq
WHERE {?note np:hasRealisation ?musicalObj .  
  ?musicalObj mop:hasFrequency ?freq.}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?literalDynamic ?midiVelocity
WHERE {?note np:hasNoteDynamic ?noteDynamic. 
  ?noteDynamic np:hasMidiVelocity ?midiVelocity 
  OPTIONAL {?noteDynamic np:hasLiteralDynamic ?literalDynamic}}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT ?note ?sustain ?decay ?release ?attack
WHERE {?note np:hasRealisation ?musicalObj .  
  ?musicalObj mop:hasEnvelope ?envelope . 
  ?envelope mop:hasSustain ?sustain;
 mop:hasDecay ?decay;
 mop:hasRelease ?release;
 mop:hasAttack ?attack.}</cpannotationschema:hasUnitTest>
        <cpannotationschema:hasUnitTest rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SELECT DISTINCT ?note ?part
WHERE {{?note np:belongsToVoice ?voice. 
   ?voice spp:isVoiceOf ?part} 
  UNION {?note np:belongsToSection ?section. 
    ?section spp:isSectionOf ?part}}</cpannotationschema:hasUnitTest>
        <cpannotationschema:reengineeredFrom rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology"/>
        <cpannotationschema:relatedCPs rdf:resource="https://purl.org/andreapoltronieri/musicalobject"/>
        <cpannotationschema:relatedCPs rdf:resource="https://purl.org/andreapoltronieri/scorepart"/>
        <rdfs:comment xml:lang="en">The Note Pattern models a music note both in relation to a music score and in relation to the musical object produced by playing such note.</rdfs:comment>
        <rdfs:label xml:lang="en">Note Pattern</rdfs:label>
        <owl:versionInfo xml:lang="en">Version 1.0</owl:versionInfo>
    </owl:Ontology>
    


